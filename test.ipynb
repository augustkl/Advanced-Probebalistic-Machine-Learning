   "source": [
    "import numpy as np\n",
    "\n",
    "X = np.array([[3, -1], [4, 2], [2, 1]])\n",
    "Y = np.array([[2], [1], [1]])\n",
    "\n",
    "print(X)\n",
    "print(Y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
