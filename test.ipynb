   "source": [
    "sig_2 = 5\n",
    "tau_2 = 0.2\n",
    "W = np.linalg.inv(X.T @ X + sig_2 / tau_2 * np.eye(2)) @ X.T @ Y\n",
    "print(W)"
   ]
